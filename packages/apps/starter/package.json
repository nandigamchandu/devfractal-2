{
  "name": "devfractal-starter",
  "version": "1.2.19",
  "private": true,
  "dependencies": {
    "formik": "1.5.8",
    "react": "16.10.2",
    "react-datepicker": "2.9.6",
    "react-dom": "16.10.2",
    "technoidentity-devfractal": "^1.2.19",
    "technoidentity-utils": "^1.2.19",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.2",
    "@types/chance": "1.0.7",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@types/yup": "0.26.24",
    "babel-eslint": "10.0.3",
    "chance": "1.1.3",
    "confusing-browser-globals": "1.0.9",
    "cross-env": "6.0.3",
    "eslint": "6.5.1",
    "jest": "24.9.0",
    "json-server": "0.15.1",
    "parcel-bundler": "1.12.4",
    "prettier": "1.18.2",
    "react-styleguidist": "9.1.16",
    "react-testing-library": "8.0.1",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.1.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "parcel index.html --open",
    "server": "json-server data/todos.json",
    "build": "parcel build index.html",
    "clean": "rimraf dist .cache",
    "test": "cross-env NODE_EN='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "tlint": "yarn tslint -p . 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "yarn types --watch",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "sg": "styleguidist server",
    "sg:build": "styleguidist build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
