{
  "name": "technoidentity-meta-client",
  "version": "0.5.9",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsdx watch",
    "test": "cross-env NODE_ENV='test' jest",
    "test:watch": "yarn test --watch",
    "build": "tsdx build",
    "lint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "types": "tsc --noEmit -p tsconfig.json",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --noEmit -p tsconfig.json --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "sg": "styleguidist server",
    "sg:build": "styleguidist build",
    "sb": "start-storybook"
  },
  "dependencies": {
    "date-fns": "1.30.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.7",
    "@types/chance": "1.0.5",
    "@types/jest": "24.0.15",
    "@types/react-dom": "16.8.5",
    "@types/react-router-dom": "4.3.4",
    "@types/react-router": "5.0.3",
    "@types/react": "16.8.23",
    "@types/yup": "0.26.22",
    "chance": "1.0.18",
    "date-fns": "1.30.1",
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "io-ts-types": "0.5.0",
    "io-ts": "2.0.0",
    "jest-cli": "24.8.0",
    "jest": "24.8.0",
    "monocle-ts": "2.0.0",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "react-router": "5.0.1",
    "react": "16.8.6",
    "technoidentity-devfractal-ui-core": "*",
    "technoidentity-devfractal-simple": "*",
    "technoidentity-devfractal-crud": "*",
    "technoidentity-meta-core": "*",
    "technoidentity-utils": "*",
    "tcomb": "3.2.29",
    "ts-jest": "24.0.2",
    "tsdx": "0.7.2",
    "yup": "0.27.0"
  },
  "peerDependencies": {
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "io-ts": "2.0.0",
    "io-ts-types": "0.5.0",
    "monocle-ts": "2.0.0",
    "tcomb": "3.2.29",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "technoidentity-devfractal-ui-core": "*",
    "technoidentity-devfractal-simple": "*",
    "technoidentity-devfractal-crud": "*",
    "technoidentity-meta-core": "*",
    "technoidentity-utils": "*",
    "yup": "0.27.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate:noFix"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
