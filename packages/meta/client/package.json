{
  "name": "meta-client",
  "version": "1.2.19",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/meta-client.esm.js",
  "typings": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "watch": "tsdx watch --tsconfig tsconfig.build.json",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p tsconfig.json",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --noEmit -p tsconfig.json --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "sg": "styleguidist server",
    "sg:build": "styleguidist build",
    "sb": "start-storybook"
  },
  "dependencies": {
    "date-fns": "2.9.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "chance": "1.1.4",
    "cross-env": "6.0.3",
    "date-fns": "2.9.0",
    "devfractal-crud": "1.2.19",
    "devfractal-simple": "1.2.19",
    "devfractal-ui-core": "1.2.19",
    "eslint": "6.8.0",
    "formik": "2.1.2",
    "jest": "24.9.0",
    "meta-core": "1.2.19",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "technoidentity-utils": "1.2.19",
    "tsdx": "0.12.3",
    "tslint": "5.20.1",
    "typescript": "3.7.5",
    "yup": "0.28.0"
  },
  "peerDependencies": {
    "devfractal-crud": "1.2.19",
    "meta-core": "1.2.5"
  }
}
