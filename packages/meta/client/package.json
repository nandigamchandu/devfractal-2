{
  "name": "meta-client",
  "version": "1.2.18",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "lib": "tsdx watch",
    "test": "cross-env NODE_ENV='test' jest",
    "test:watch": "yarn test --watch",
    "build": "tsdx build",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p tsconfig.json",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --noEmit -p tsconfig.json --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "sg": "styleguidist server",
    "sg:build": "styleguidist build",
    "sb": "start-storybook"
  },
  "dependencies": {
    "date-fns": "2.0.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "chance": "1.0.18",
    "date-fns": "2.0.1",
    "devfractal-crud": "1.2.18",
    "devfractal-simple": "1.2.18",
    "devfractal-ui-core": "1.2.18",
    "eslint": "6.3.0",
    "formik": "1.5.8",
    "technoidentity-spec": "*",
    "jest": "24.9.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "technoidentity-meta-core": "1.2.18",
    "technoidentity-utils": "1.2.18",
    "cross-env": "5.2.0",
    "lint-staged": "9.2.5",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "tslint": "5.19.0",
    "typescript": "3.5.3",
    "tsdx": "0.9.0",
    "yup": "0.27.0"
  },
  "peerDependencies": {
    "devfractal-crud": "1.2.18",
    "technoidentity-meta-core": "1.2.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
