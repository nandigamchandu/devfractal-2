{
  "name": "demo-presentation",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "bulma": "0.7.5",
    "date-fns": "1.30.1",
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "immer": "3.1.3",
    "io-ts": "2.0.0",
    "react": "16.8.6",
    "react-datepicker": "2.8.0",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-use": "10.3.0",
    "technoidentity-devfractal": "^1.2.0",
    "technoidentity-utils": "^1.2.0",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.7",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-datepicker": "2.8.1",
    "@types/react-dom": "16.8.5",
    "@types/react-router-dom": "4.3.4",
    "@types/yup": "0.26.22",
    "cross-env": "5.2.0",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "json-server": "0.15.0",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "server": "json-server data/todos.json",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "lint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "resolutions": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate:noFix"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
