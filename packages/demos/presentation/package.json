{
  "name": "demo-presentation",
  "version": "1.2.18",
  "private": true,
  "dependencies": {
    "date-fns": "2.0.1",
    "formik": "1.5.8",
    "immer": "3.2.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "technoidentity-devfractal": "1.2.18",
    "technoidentity-spec": "*",
    "technoidentity-utils": "1.2.18",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "bulma": "0.7.5",
    "cross-env": "5.2.0",
    "eslint": "6.3.0",
    "husky": "3.0.4",
    "jest": "24.9.0",
    "json-server": "0.15.0",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "react-datepicker": "2.9.3",
    "rimraf": "3.0.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "server": "json-server data/todos.json",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "lint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "types": "tsc --noEmit -p .",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
