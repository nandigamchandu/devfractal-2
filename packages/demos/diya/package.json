{
  "name": "demo-diya-client",
  "version": "1.2.18",
  "private": true,
  "scripts": {
    "cypress": "cypress open",
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "server": "cross-env PORT=9999 ts-node --project tsconfig.tsnode.json src/db.ts",
    "build": "parcel build index.html",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "date-fns": "2.0.1",
    "formik": "1.5.8",
    "lodash-es": "4.17.15",
    "react-dom": "16.9.0",
    "react": "16.9.0",
    "technoidentity-devfractal": "1.2.18",
    "technoidentity-spec": "*",
    "technoidentity-utils": "1.2.18",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^4.2.0",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.3",
    "@types/jest": "24.0.18",
    "@types/lodash-es": "4.17.3",
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "@types/yup": "0.26.23",
    "cross-env": "5.2.0",
    "cypress": "^3.4.1",
    "eslint": "6.3.0",
    "husky": "3.0.4",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react-datepicker": "2.9.3",
    "rimraf": "3.0.0",
    "technoidentity-dev-utils": "1.2.18",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "tslint": "5.19.0",
    "typescript-tslint-plugin": "0.5.4",
    "typescript": "3.5.3"
  },
  "proxy": "http://localhost:9999",
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
