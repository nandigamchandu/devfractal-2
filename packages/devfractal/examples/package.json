{
  "name": "devfractal-examples",
  "version": "0.0.1",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/devfractal-examples.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "parcel index.html --open",
    "lib": "tsdx watch",
    "build": "tsdx build",
    "test": "cross-env NODE_ENV='test' jest",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.20",
    "@fortawesome/free-brands-svg-icons": "5.10.0",
    "@fortawesome/free-regular-svg-icons": "5.10.0",
    "@fortawesome/free-solid-svg-icons": "5.10.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "date-fns": "1.30.1",
    "tcomb": "3.2.29",
    "devfractal-api": "*",
    "devfractal-crud": "*",
    "devfractal-forms": "*",
    "devfractal-router": "*",
    "devfractal-simple": "*",
    "devfractal-ui": "*",
    "devfractal-ui-core": "*",
    "technoidentity-utils": "*",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.7",
    "@types/chance": "1.0.5",
    "@types/cleave.js": "1.4.0",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "@types/react-test-renderer": "16.8.3",
    "@types/yup": "0.26.22",
    "firebase": "6.3.4",
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "husky": "3.0.2",
    "io-ts": "2.0.0",
    "io-ts-types": "0.5.0",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "monocle-ts": "2.0.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-test-render": "1.1.2",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "peerDependencies": {
    "firebase": "6.3.4",
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "io-ts": "2.0.0",
    "io-ts-types": "2.0.0",
    "monocle-ts": "2.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "yup": "0.27.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate:noFix"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
