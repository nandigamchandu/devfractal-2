{
  "name": "devfractal-router",
  "version": "1.2.18",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/devfractal-router.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "lib": "tsdx watch",
    "build": "tsdx build",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "query-string": "6.8.2",
    "technoidentity-utils": "^1.2.18",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.3",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.5",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "babel-eslint": "10.0.2",
    "eslint": "6.2.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "fp-ts": "2.0.5",
    "husky": "3.0.4",
    "io-ts": "2.0.1",
    "io-ts-types": "0.5.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "lint-staged": "9.2.3",
    "monocle-ts": "2.0.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "rimraf": "3.0.0",
    "tsdx": "0.8.0",
    "yup": "0.27.0"
  },
  "peerDependencies": {
    "fp-ts": "2.0.5",
    "io-ts": "2.0.1",
    "io-ts-types": "0.5.0",
    "monocle-ts": "2.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
