{
  "name": "next-site",
  "version": "1.2.13",
  "private": true,
  "dependencies": {
    "bulma": "0.7.5",
    "devfractal-examples": "1.2.13",
    "devfractal-ui-core": "1.2.13",
    "fp-ts": "2.0.5",
    "io-ts": "2.0.1",
    "monocle-ts": "2.0.0",
    "next": "9.0.3",
    "technoidentity-utils": "1.2.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.0.0",
    "@types/jest": "24.0.17",
    "@types/next": "8.0.6",
    "@types/node": "12.7.1",
    "@types/yup": "0.26.22",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "babel-eslint": "10.0.2",
    "cross-env": "5.2.0",
    "eslint": "6.1.0",
    "husky": "3.0.3",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "react-testing-library": "8.0.1",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "dev": "next start",
    "build": "next build",
    "start": "next",
    "clean": "rimraf dist .cache",
    "test": "cross-env NODE_EN='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "tlint": "yarn tslint -p . 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './pages/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "yarn types --watch",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "validate:noFix": "yarn lint && yarn types && yarn test"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
