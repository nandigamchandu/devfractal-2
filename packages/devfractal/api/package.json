{
  "name": "devfractal-api",
  "version": "1.2.5",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/devfractal-api.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "server": "ts-node-dev --project tsconfig.tsnode.json src/server.ts",
    "lib": "tsdx watch",
    "build": "tsdx build",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "axios": "0.19.0",
    "immer": "3.2.0",
    "io-ts-promise": "2.0.0",
    "query-string": "6.8.2",
    "tslib": "1.10.0",
    "typelevel-ts": "0.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.9",
    "@types/jest": "24.0.17",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.8.5",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "^10.0.2",
    "devfractal-router": "^1.2.5",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "formik": "1.5.8",
    "fp-ts": "2.0.4",
    "husky": "3.0.3",
    "io-ts": "2.0.1",
    "io-ts-types": "0.5.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "lint-staged": "9.2.1",
    "monocle-ts": "2.0.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "rimraf": "2.6.3",
    "technoidentity-dev-utils": "^1.2.5",
    "technoidentity-utils": "^1.2.5",
    "ts-node-dev": "1.0.0-pre.40",
    "tsdx": "0.7.2"
  },
  "peerDependencies": {
    "devfractal-router": "*",
    "formik": "1.5.8",
    "fp-ts": "2.0.4",
    "io-ts": "2.0.1",
    "io-ts-promise": "2.0.0",
    "io-ts-types": "0.5.0",
    "monocle-ts": "2.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "technoidentity-utils": "*"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
