{
  "name": "devfractal-site",
  "version": "1.2.18",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p tsconfig.json",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --noEmit -p tsconfig.json --watch",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "validate:noFix": "yarn lint:fix && yarn types && yarn test",
    "prebuild": "yarn validate",
    "sg": "styleguidist server",
    "sg:build": "styleguidist build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.22",
    "@fortawesome/free-brands-svg-icons": "5.10.2",
    "@fortawesome/free-regular-svg-icons": "5.10.2",
    "@fortawesome/free-solid-svg-icons": "5.10.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "devfractal-api": "1.2.18",
    "devfractal-crud": "1.2.18",
    "devfractal-examples": "1.2.18",
    "devfractal-forms": "1.2.18",
    "devfractal-router": "1.2.18",
    "devfractal-simple": "1.2.18",
    "devfractal-ui": "1.2.18",
    "devfractal-ui-api": "1.2.18",
    "devfractal-ui-core": "1.2.18",
    "technoidentity-spec": "*",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "technoidentity-utils": "1.2.18"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.3",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@types/react-router": "5.0.3",
    "@types/react-test-renderer": "16.9.0",
    "@types/react": "16.9.2",
    "babel-eslint": "10.0.3",
    "cross-env": "5.2.0",
    "eslint": "6.3.0",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "react-test-renderer": "16.9.0",
    "rimraf": "3.0.0",
    "ts-jest": "24.0.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3"
  },
  "resolutions": {
    "react": "16.9.0",
    "react-dom": "16.9.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
