{
  "name": "devfractal-site",
  "version": "1.2.2",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "build": "parcel build index.html",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "lint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "types": "tsc --noEmit -p tsconfig.json",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --noEmit -p tsconfig.json --watch",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "validate:noFix": "yarn lint:fix && yarn types && yarn test",
    "prebuild": "yarn validate"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.20",
    "@fortawesome/free-brands-svg-icons": "5.10.0",
    "@fortawesome/free-regular-svg-icons": "5.10.0",
    "@fortawesome/free-solid-svg-icons": "5.10.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "devfractal-api": "^1.2.2",
    "devfractal-crud": "^1.2.2",
    "devfractal-examples": "^1.2.2",
    "devfractal-forms": "^1.2.2",
    "devfractal-router": "^1.2.2",
    "devfractal-simple": "^1.2.2",
    "devfractal-ui": "^1.2.2",
    "devfractal-ui-core": "^1.2.2",
    "fp-ts": "2.0.3",
    "io-ts": "2.0.0",
    "io-ts-types": "0.5.0",
    "monocle-ts": "2.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "technoidentity-utils": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.7",
    "@types/jest": "24.0.16",
    "@types/node": "12.6.9",
    "@types/react": "16.8.24",
    "@types/react-dom": "16.8.5",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "@types/react-test-renderer": "16.8.3",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "0.5.4"
  },
  "resolutions": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
