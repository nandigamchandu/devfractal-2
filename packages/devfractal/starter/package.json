{
  "name": "devfractal-starter",
  "version": "1.2.18",
  "private": true,
  "dependencies": {
    "formik": "1.5.8",
    "fp-ts": "2.0.5",
    "io-ts": "2.0.1",
    "io-ts-types": "0.5.0",
    "monocle-ts": "2.0.0",
    "react": "16.9.0",
    "react-datepicker": "2.8.0",
    "react-dom": "16.9.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "technoidentity-devfractal": "^1.2.18",
    "technoidentity-utils": "^1.2.18",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.0",
    "@types/chance": "1.0.6",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "@types/yup": "0.26.23",
    "babel-eslint": "10.0.3",
    "chance": "1.0.18",
    "confusing-browser-globals": "1.0.8",
    "cross-env": "5.2.0",
    "eslint": "6.2.2",
    "husky": "3.0.4",
    "jest": "24.9.0",
    "json-server": "0.15.0",
    "lint-staged": "9.2.5",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react-testing-library": "8.0.1",
    "rimraf": "3.0.0",
    "ts-jest": "24.0.2",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.6.2",
    "typescript-tslint-plugin": "0.5.4"
  },
  "scripts": {
    "precommit": "yarn lint-staged",
    "prepush": "yarn validate:noFix",
    "start": "parcel index.html --open",
    "server": "json-server data/todos.json",
    "build": "parcel build index.html",
    "clean": "rimraf dist .cache",
    "test": "cross-env NODE_EN='test' jest",
    "test:watch": "yarn test --watch",
    "tlint": "yarn tslint -p . 'src/**/*.tsx' 'src/**/*.ts'",
    "tlint:fix": "yarn lint --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn elint && yarn tlint",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "yarn types --watch",
    "validate": "yarn lint:fix && yarn types && yarn test",
    "validate:noFix": "yarn lint && yarn types && yarn test"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
