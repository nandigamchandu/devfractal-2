{
  "name": "site-devfractal",
  "version": "1.2.19",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "parcel build index.html",
    "elint:fix": "yarn elint --quiet --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "lint": "yarn elint && yarn tlint",
    "prebuild": "yarn validate",
    "sg:build": "styleguidist build",
    "sg": "styleguidist server",
    "start": "parcel index.html --open",
    "test:watch": "yarn test --watch",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "tlint:fix": "yarn lint --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling tsc --noEmit -p tsconfig.json --watch",
    "types": "tsc --noEmit -p tsconfig.json",
    "validate:noFix": "yarn lint:fix && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "devfractal-api": "^1.2.19",
    "devfractal-crud": "^1.2.19",
    "devfractal-examples": "^1.2.19",
    "devfractal-forms": "^1.2.19",
    "devfractal-router": "^1.2.19",
    "devfractal-simple": "^1.2.19",
    "devfractal-ui": "^1.2.19",
    "devfractal-ui-api": "^1.2.19",
    "devfractal-ui-core": "^1.2.19",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "technoidentity-utils": "^1.2.19"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/react": "9.3.0",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.2",
    "@types/react": "16.9.9",
    "@types/react-dom": "16.9.2",
    "@types/react-router": "5.1.2",
    "@types/react-router-dom": "5.1.0",
    "@types/react-test-renderer": "16.9.1",
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.3",
    "eslint": "6.5.1",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.10.2",
    "rimraf": "3.0.0",
    "ts-jest": "24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.1.0",
    "typescript": "3.6.4"
  },
  "resolutions": {
    "react": "16.10.2",
    "react-dom": "16.10.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
