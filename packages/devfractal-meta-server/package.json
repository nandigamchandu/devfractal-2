{
  "name": "@technoidentity/meta-server",
  "version": "0.0.1",
  "description": "Meta utilities for server.",
  "main": "build/main.js",
  "typings": "build/index.d.ts",
  "author": "Pervez Iqbal",
  "private": true,
  "scripts": {
    "build": "backpack build",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "clean": "rimraf build",
    "clean:dist": "rimraf node_modules yarn.lock",
    "refresh": "yarn clean && yarn clean:dist && yarn",
    "lint:cmd": "tslint -p tsconfig.json -c tslint.json",
    "lint": "yarn lint:cmd 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "yarn types --watch",
    "validate": "yarn lint && yarn types && yarn test"
  },
  "dependencies": {
    "@technoidentity/meta-core": "0.0.1",
    "@technoidentity/utils": "0.0.1",
    "express": "4.17.1",
    "fp-ts": "1.19.0",
    "io-ts": "1.9.0",
    "io-ts-types": "0.4.7",
    "mongodb": "3.2.7",
    "mongoose": "5.6.0",
    "monocle-ts": "1.7.2",
    "tcomb": "3.2.29",
    "tcomb-validation": "3.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "4.17.0",
    "@types/jest": "24.0.15",
    "@types/mongodb": "3.1.28",
    "@types/mongoose": "5.5.6",
    "babel-jest": "^24.8.0",
    "backpack-core": "^0.8.3",
    "cross-env": "^5.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2"
  }
}
