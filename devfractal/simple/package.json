{
  "name": "technoidentity-devfractal-simple",
  "version": "0.10.0",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/technoidentity-devfractal-simple.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lib": "tsdx watch",
    "start": "parcel index.html --open",
    "build": "tsdx build",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "elint:fix": "yarn elint --quiet --fix",
    "lint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix",
    "types": "tsc --noEmit -p .",
    "types:watch": "yarn types --watch",
    "validate": "yarn lint && yarn types && yarn test"
  },
  "dependencies": {
    "technoidentity-utils": "*",
    "technoidentity-devfractal-ui-core": "*",
    "technoidentity-devfractal-ui": "*",
    "technoidentity-devfractal-forms": "*",
    "technoidentity-devfractal-router": "*",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.0.0",
    "@testing-library/react": "8.0.7",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@types/react-router": "5.0.3",
    "@types/react-router-dom": "4.3.4",
    "@types/yup": "0.26.22",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "husky": "3.0.1",
    "io-ts": "2.0.0",
    "io-ts-types": "0.5.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "lint-staged": "9.2.1",
    "monocle-ts": "2.0.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "rimraf": "2.6.3",
    "tsdx": "0.7.2",
    "yup": "0.27.0"
  },
  "peerDependencies": {
    "formik": "1.5.8",
    "fp-ts": "2.0.3",
    "io-ts": "2.0.0",
    "io-ts-promise": "2.0.0",
    "io-ts-types": "0.5.0",
    "monocle-ts": "2.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "yup": "0.27.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ],
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
