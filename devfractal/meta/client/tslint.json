{
  "extends": ["tslint-react", "../../tslint.base.json"],
  "rules": {
    "file-name-casing": [true, { ".tsx": "pascal-case", ".ts": "camel-case" }],
    "jsx-boolean-value": false,
    "jsx-key": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-no-string-ref": true,
    "jsx-wrap-multiline": false
  }
}
